<style lang="less">
.container{
   width: 100%;
   background: #fafafa url(data:image/png;base64,);
   padding-bottom:100rpx;
  .header{
    background:rgba(255,255,255,0.8);
    border-bottom:1rpx solid #eee;
    width:90%;
    padding:24rpx 5%;
    position:fixed;    
    .title{
      font-size: 14pt;
      padding-left: 70rpx;   
      background: url(data:image/png;base64,) no-repeat left center;
      background-size: 50rpx 50rpx;
    }
    .search{
      width:50rpx;
      height:50rpx;
      position:absolute;
      right:16rpx;
      top:24rpx;
    }
  }
  .info{
    color:gray;
    opacity:.8;
    margin:0 auto 20rpx auto;
    &.loading{
      width: 70rpx;
      height: 70rpx;
    }
  }
}
.pickers{background: rgba(255,255,255,0.3); padding: 30rpx 16rpx;border-bottom: 1rpx dotted #ccc;margin-top:100rpx;}
.pickers .tabs{color: #666;margin-left: 20rpx;font-size: 10pt;padding:4rpx 16rpx;background: #eee;border-radius:10rpx}
.pickers .tabs.active{color: #fff;background: #d9220e !important}
.pickers .tabs:nth-child(1){margin-left: 0px;}
</style>
<template>
  <view class="container">
    <view class="header">
        <view class="title">
          <text>目录</text>
        </view>
        <navigator url="/pages/search">
          <image class="search" src="../assets/search.png"></image>
        </navigator>
    </view>
    <view class="pickers">
      <repeat for="{{tab}}" >
        <text class="tabs {{author == item?'active':''}}" @tap="changeV({{item}})">{{item}}</text>
      </repeat>
    </view>
    <view>
    <repeat for="{{detail.objects}}" >
      <view @tap="tap({{item}})" wx:if="{{item.id}}"> 
        <list :title="item.title" :author="item.author" />      
      </view>
    </repeat>
    </view>
    <text class="info" wx:if="{{info}}">{{info}}</text>
    <image wx:else class="info loading" src="../assets/loading.gif"/>
  </view>
</template>

<script>
  import wepy from 'wepy'
  import List from '../components/poemlist' 

  export default class Index extends wepy.page {
    config = {
      navigationBarTitleText: '全部诗集',
      window: {
        enablePullDownRefresh: true
      }
    }
    components = {
      list: List
    }
    data = {
      detail: {
        objects: []
      },
      isLoadingMore: false,
      currentPage: 1,
      info: '',
      tab: ['李白', '李清照', '王維', '孟浩然', '杜甫'],
      author: '李清照'
    }
    methods = {
      tap(items) {
        wx.setStorage({
          key:"listdata",
          data:items
        })
        wx.navigateTo({
          url: `/pages/detail?id=${items.id}`  // 打开一个新的同路由页面，但指定不同的数据初始值
        })
      },
      changeV(items){
        this.author = items;
        this.detail.objects = [];
        wx.showLoading({
          title: '加载中'
        })  
        this.loadList()
      }
    }
    onLoad() {
      wx.showLoading({
        title: '加载中'
      })
      this.loadList()
    }
    onPullDownRefresh() {      
      wx.stopPullDownRefresh()
    }
    onReachBottom() {
      this.currentPage++
      if (this.currentPage >= 10) { // 最多只能加载10页
        this.isLoadingMore = false
        this.info = 'No more news'
        this.$apply()
        return
      }
      this.isLoadingMore = true
      this.loadList()
    }
    loadList() {
      var that = this
      wx.request({
        url: `https://www.hashimao.com/poems/poets/?page=${that.currentPage}&author=${that.author}`,
        success: (res) => {
          if (res.statusCode == 200) {
            that.detail.objects = that.detail.objects.concat(res.data)
          }       
          that.$apply()
          wx.hideLoading()
        }
      })
    }  
  }
</script>
